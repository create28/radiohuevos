{
  "version": 2,
  "name": "radio-huevos",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/playlist-detail.html",
      "dest": "/playlist-detail.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
